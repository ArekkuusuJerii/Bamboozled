{
  "multipart": [
    { "when": { "up": "true" }, "apply": { "model": "bamboozled:bamboo" } },
    { "when": { "up": "false" }, "apply": { "model": "bamboozled:bamboo_top" } },
    { "when": { "north": "true" }, "apply": { "model": "bamboozled:bamboo_north" } },
    { "when": { "south": "true" }, "apply": { "model": "bamboozled:bamboo_south" } },
    { "when": { "west": "true" }, "apply": { "model": "bamboozled:bamboo_west" } },
    { "when": { "leaves": "1" }, "apply": { "model": "bamboozled:bamboo_leaves_0" } },
    { "when": { "leaves": "2" }, "apply": { "model": "bamboozled:bamboo_leaves_1" } },
    { "when": { "leaves": "3" }, "apply": { "model": "bamboozled:bamboo_leaves_2" } }
  ]
}