{
    "multipart": [
        {
            "apply": { "model": "bamboozled:bamboo_wall" }
        },
        {
            "when": { "north": "false" },
            "apply": { "model": "bamboozled:bamboo_wall_none", "y": 0, "uvlock": true }
        },
        {
            "when": { "south": "false" },
            "apply": { "model": "bamboozled:bamboo_wall_none", "y": 180, "uvlock": true }
        },
        {
            "when": { "west": "false" },
            "apply": { "model": "bamboozled:bamboo_wall_none", "y": 270, "uvlock": true }
        },
        {
            "when": { "east": "false" },
            "apply": { "model": "bamboozled:bamboo_wall_none", "y": 90, "uvlock": true }
        },
        {
            "when": { "north": "true" },
            "apply": { "model": "bamboozled:bamboo_wall_side", "y": 0, "uvlock": true }
        },
        {
            "when": { "south": "true" },
            "apply": { "model": "bamboozled:bamboo_wall_side", "y": 180, "uvlock": true }
        },
        {
            "when": { "west": "true" },
            "apply": { "model": "bamboozled:bamboo_wall_side", "y": 270, "uvlock": true }
        },
        {
            "when": { "east": "true" },
            "apply": { "model": "bamboozled:bamboo_wall_side", "y": 90, "uvlock": true }
        }
    ]
}